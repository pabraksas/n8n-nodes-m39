{
  "name": "n8n-nodes-m39",
  "version": "0.1.3",
  "description": "media39",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://media39.ru",
  "author": {
    "name": "Pavel Abraksas",
    "email": "Pavel.Abraksas@gmailcom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabraksas/n8n-nodes-m39.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly0": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Petstore/Petstore.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.4"
  }
